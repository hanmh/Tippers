package assignment9;

import java.awt.Color;

public class ThreeDimenaionalShape extends Shape{

	private float volume;

	public ThreeDimenaionalShape(String shape, Color color)
	{
		super(shape, color);
	}
	
	public float calculate()	// 나중에 오버라이딩 하여 사용할 함수이다.
	{
		return 0;
	}
	
	public void setVolume(float volume)
	{
		this.volume = volume;
	}
	
	public float getVolume()
	{
		return this.volume;
	}
}
