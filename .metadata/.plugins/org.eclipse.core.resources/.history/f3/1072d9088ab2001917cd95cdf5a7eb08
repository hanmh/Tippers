package uci.dbh.isg.test;

import uci.dbh.isg.wrapper.*;
import uci.dbh.isg.wrapper.connectors.*;
import uci.dbh.isg.wrapper.databuilders.*;
import uci.dbh.isg.wrapper.models.*;

public class TestOfWrapper{
	
	public static void main(String[] args) {
		
		TemperatureObservation tempOb;
		Sensor sensor;
		InteractionType interactionType;
		LLRequest request;
		
		WrapperComponent wc = new WrapperComponent();
		
		tempOb = new TemperatureObservation("label", "scale", "1");
		sensor = new Sensor("id", "address", "port");
		sensor.setTempOb(tempOb);
		interactionType = new InteractionType(InteractionType.REQUEST);
		request = new LLRequest(sensor, "requestData", 500, 10000, interactionType);
		
		wc.request(request);
		
		
		try { Thread.sleep(1000); } catch (InterruptedException e){}
		tempOb = new TemperatureObservation("label", "scale", "10");
		sensor = new Sensor("id", "address", "port");
		interactionType = new InteractionType(InteractionType.STREAMING);
		request = new LLRequest(sensor, "requestData", 2000, 10000, interactionType);
		
		
		try { Thread.sleep(1000); } catch (InterruptedException e){}
		tempOb = new TemperatureObservation("label", "scale", "100");
		sensor = new Sensor("id", "address", "port");
		interactionType = new InteractionType(InteractionType.STREAMING);
		request = new LLRequest(sensor, "requestData", 500, 10000, interactionType);
		
	}
}
